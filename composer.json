{
    "name": "yuloh/stream",
    "type": "library",
    "description": "A stream implementation for PSR-7",
    "keywords": [
        "yuloh",
        "stream"
    ],
    "homepage": "https://github.com/yuloh/stream",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Allan",
            "email": "matthew.james.allan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer": "~2.3",
        "php-mock/php-mock": "^1.0",
        "scrutinizer/ocular": "~1.3",
        "zendframework/zend-diactoros": "^1.3",
        "guzzlehttp/psr7": "^1.3",
        "slim/slim": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Yuloh\\Stream\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuloh\\Stream\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs --standard=psr2 src/"
    }
}
